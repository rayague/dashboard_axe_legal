{
  "version": 2,
  "framework": null,
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    { "src": "/build/(.*)", "dest": "/public/build/$1" },
    { "src": "/favicon.ico", "dest": "/public/favicon.ico" },
    { "src": "/robots.txt", "dest": "/public/robots.txt" },
    { "src": "/(css|js)/(.*)", "dest": "/public/$1/$2" },
    { "src": "/(.*)", "dest": "/api/vercel.php" }
  ],
  "env": {
    "APP_NAME": "AxeLegal",
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "APP_URL": "https://dashboard-axe-legal.vercel.app",
    "VERCEL_DEMO": "true",
    "APP_CONFIG_CACHE": "/tmp/config.php",
    "APP_EVENTS_CACHE": "/tmp/events.php",
    "APP_PACKAGES_CACHE": "/tmp/packages.php",
    "APP_ROUTES_CACHE": "/tmp/routes.php",
    "APP_SERVICES_CACHE": "/tmp/services.php",
    "VIEW_COMPILED_PATH": "/tmp/views",
    "CACHE_DRIVER": "array",
    "LOG_CHANNEL": "stderr",
    "SESSION_DRIVER": "cookie",
    "SSR_TEMP_PATH": "/tmp/ssr",
    "NODE_PATH": "node"
  },
  "buildCommand": "chmod +x ./vercel-install.sh && ./vercel-install.sh && npm run build",
  "outputDirectory": "public"
}
