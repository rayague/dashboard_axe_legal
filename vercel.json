{
  "version": 2,
  "functions": {
    "api/*.php": {
      "runtime": "edouarddemotesq/php@1.0.0",
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ],
  "build": {
    "env": {
      "PHP_VERSION": "8.2"
    }
  }
}
